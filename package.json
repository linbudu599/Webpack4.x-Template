{
  "name": "webpackrestart",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development &&  webpack  --config ./build/webpack.dev.js --progress",
    "serve": "cross-env NODE_ENV=development && webpack-dev-server --config ./build/webpack.dev.js",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext  .vue src/",
    "analyze": "cross-env ANALYZE=1 NODE_ENV=production && webpack --progress --config ./build/webpack.prod.js --json > stats.json",
    "build": "cross-env NODE_ENV=production && webpack --progress --config ./build/webpack.prod.js",
    "build:dll": "webpack --config ./build/webpack.dll.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint -c .eslintrc.js"
    ],
    "*.scss": [
      "stylelint --config .stylelintrc.json"
    ],
    "*.{ts,tsx,js,json,scss,md}": [
      "prettier --write"
    ]
  },
  "author": "linbudu",
  "license": "MIT",
